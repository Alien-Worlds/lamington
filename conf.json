{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [
            "./lib"
        ],
        "exclude": [
            "./src"
        ],
        "includePattern": ".+\\.(js)x?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs/"
    }
}