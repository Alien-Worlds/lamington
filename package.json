{
	"name": "lamington",
	"version": "1.1.0-alpha.6",
	"description": "A suite of tools to make building and testing EOS smart contracts easier. ",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "tsc && npm run copy:scripts && npm run copy:configs",
		"changelog": "auto-changelog --commit-limit false",
		"check:types": "tsc --noEmit",
		"copy:scripts": "cp -r ./src/scripts/ ./lib/scripts/",
		"copy:configs": "cp -R ./src/eosio-config/ ./lib/eosio-config/; cp -R ./eosio-contracts ./lib/eosio-contracts",
		"clean": "rm -rf ./lib",
		"docs": "typedoc --out api-docs ./src",
		"format": "prettier --write \"src/**/*.ts\"",
		"lamington:init": "lamington init",
		"prepublishOnly": "npm test",
		"prepare": "npm run build",
		"test": "TS_NODE_FILES=true mocha --require ts-node/register src/**/*.test.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CoinageCrypto/lamington.git"
	},
	"bin": {
		"lamington": "./lib/cli/index.js"
	},
	"keywords": [
		"EOS",
		"EOS.io",
		"Smart Contract",
		"C++",
		"wasm",
		"Typescript",
		"Truffle",
		"Compile",
		"Deployment",
		"Testing",
		"Unit Testing",
		"Mocha"
	],
	"author": "Kevin Brown",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CoinageCrypto/lamington/issues"
	},
	"homepage": "https://github.com/CoinageCrypto/lamington#readme",
	"files": [
		"lib/**/*"
	],
	"devDependencies": {
		"@types/chai": "4.3.20",
		"@types/chalk": "2.2.0",
		"@types/mkdirp": "1.0.1",
		"@types/mocha": "10.0.10",
		"@types/ncp": "2.0.4",
		"@types/node": "14.0.27",
		"@types/node-fetch": "2.6.9",
		"@types/ora": "3.2.0",
		"@types/rimraf": "3.0.0",
		"@types/text-encoding": "0.0.35",
		"auto-changelog": "2.5.0",
		"eosjs-ecc": "4.0.7",
		"mocha": "11.7.5",
		"prettier": "2.0.5",
		"ts-mocha": "11.1.0",
		"ts-node": "10.9.2",
		"tsc-watch": "7.2.0",
		"typedoc": "^0.20.34",
		"typescript": "4.9.5"
	},
	"dependencies": {
		"@types/chai-as-promised": "^7.1.3",
		"@types/elliptic": "^6.4.12",
		"axios": "^1.13.2",
		"chai": "4.5.0",
		"chai-as-promised": "^7.1.1",
		"chalk": "4.1.0",
		"clarify": "2.1.0",
		"collections": "^5.1.13",
		"colors": "1.4.0",
		"commander": "6.0.0",
		"deep-equal-in-any-order": "^1.1.18",
		"docker-cli-js": "https://github.com/Alien-Worlds/docker-cli-js",
		"dotenv": "8.2.0",
		"eosjs": "^22.1.0",
		"glob": "7.1.6",
		"got": "11.8.5",
		"mkdirp": "1.0.4",
		"ncp": "2.0.0",
		"node-fetch": "2.6.7",
		"ora": "5.0.0",
		"qrcode-terminal": "0.12.0",
		"rimraf": "3.0.2",
		"tar": "^6.2.1",
		"tar-stream": "^2.2.0",
		"trace": "3.1.1"
	},
	"resolutions": {
		"@babel/runtime": "7.26.10",
		"base-x": "3.0.11",
		"cipher-base": "1.0.5",
		"elliptic": "6.6.1",
		"marked": "4.0.10",
		"sha.js": "2.4.12"
	},
	"peerDependencies": {
		"eosjs": "^20.0.0",
		"eosjs-ecc": "^4.0.4"
	},
	"packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
