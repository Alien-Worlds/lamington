{
	"name": "lamington",
	"version": "1.0.0",
	"description": "A suite of tools to make building and testing EOS smart contracts easier. ",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "npm run clean && tsc && npm run copy:scripts",
		"copy:scripts": "cp -r ./src/scripts/ ./lib/scripts/",
		"clean": "rm -rf ./lib",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"prepublishOnly": "npm test",
		"prepare": "npm run build",
		"test": "jest --config jestconfig.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TechInSite/lamington.git"
	},
	"bin": {
		"lamington": "./lib/cli/index.js"
	},
	"keywords": [
		"EOS",
		"Smart",
		"Contract",
		"Truffle"
	],
	"author": "Kevin Brown",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TechInSite/lamington/issues"
	},
	"homepage": "https://github.com/TechInSite/lamington#readme",
	"files": [
		"lib/**/*"
	],
	"devDependencies": {
		"@types/mkdirp": "0.5.2",
		"@types/mocha": "5.2.6",
		"@types/node": "11.13.0",
		"@types/node-fetch": "2.3.0",
		"@types/rimraf": "2.0.2",
		"@types/text-encoding": "0.0.35",
		"eosjs": "20.0.0",
		"eosjs-ecc": "4.0.4",
		"typescript": "3.4.1"
	},
	"dependencies": {
		"axios": "0.18.0",
		"commander": "2.20.0",
		"docker-cli-js": "2.5.2",
		"glob": "7.1.3",
		"mkdirp": "0.5.1",
		"mocha": "6.0.2",
		"node-fetch": "2.3.0",
		"qrcode-terminal": "0.12.0",
		"rimraf": "2.6.3",
		"ts-mocha": "6.0.0"
	},
	"peerDependencies": {
		"eosjs": "^20.0.0",
		"eosjs-ecc": "^4.0.4"
	}
}
