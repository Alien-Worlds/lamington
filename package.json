{
	"name": "lamington",
	"version": "1.0.0-alpha.3",
	"description": "A suite of tools to make building and testing EOS smart contracts easier.",
	"private": true,
	"scripts": {
		"build": "npm run clean && lerna bootstrap",
		"copy:scripts": "cp -r ./src/scripts/ ./lib/scripts/",
		"clean": "rm -rf ./lib",
		"docs": "typedoc --out api-docs ./src",
		"format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
		"prepublishOnly": "npm test",
		"prepare": "npm run build",
		"test": "mocha --require ts-node/register src/**/*.test.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CoinageCrypto/lamington.git"
	},
	"bin": {
		"lamington": "./lib/cli/index.js"
	},
	"keywords": [
		"EOS",
		"EOS.io",
		"Smart Contract",
		"C++",
		"wasm",
		"Typescript",
		"Truffle",
		"Compile",
		"Deployment",
		"Testing",
		"Unit Testing",
		"Mocha"
	],
	"contributors": [
		{
			"name": "Kevin Brown",
			"email": "kevin.brown@coina.ge",
			"url": "https://github.com/thekevinbrown"
		},
		{
			"name": "Mitch Pierias",
			"email": "mitch@pierias.com",
			"url": "https://github.com/MitchPierias"
		}
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CoinageCrypto/lamington/issues"
	},
	"homepage": "https://github.com/CoinageCrypto/lamington#readme",
	"devDependencies": {
		"@types/chai": "4.1.7",
		"@types/mocha": "5.2.7",
		"eosjs": "20.0.0",
		"eosjs-ecc": "4.0.4",
		"lerna": "^3.16.4",
		"prettier": "1.18.2",
		"ts-node": "8.3.0",
		"typedoc": "0.14.2",
		"typescript": "3.5.2"
	},
	"dependencies": {
		"chai": "4.2.0",
		"colors": "1.3.3",
		"dotenv": "8.0.0",
		"mocha": "6.1.4",
		"ts-mocha": "6.0.0"
	},
	"peerDependencies": {
		"eosjs": "^20.0.0",
		"eosjs-ecc": "^4.0.4"
	},
	"useWorkspaces": true,
	"workspaces": [
		"packages/*"
	]
}
